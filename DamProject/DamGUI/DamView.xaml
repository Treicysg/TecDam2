<Window x:Class="DamProject.DamGUI.DamView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dam Tec" Height="600" Width="800">
    <Grid Margin="10,10,2,-54">
        <Grid.RowDefinitions>
            <RowDefinition Height="153*"/>
            <RowDefinition Height="431*"/>
            <RowDefinition Height="39*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="WaterReservoirHeight" HorizontalAlignment="Left" Height="31" Margin="34,110,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="145" RenderTransformOrigin="0.711,0.235"/>
        <TextBox x:Name="txtWaterHeight" HorizontalAlignment="Left" Height="30" Margin="34,25,0,0" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="145"/>
        <TextBox x:Name="megawattstxt" HorizontalAlignment="Left" Height="37" Margin="256,255,0,0" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="176"/>
        <TextBox x:Name="riverHeighttxt" HorizontalAlignment="Left" Height="37" Margin="645,97,0,0" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
